    "import re\n",
    "mail_checking_regex = '^\\w+([\\.-]?\\w+)*@\\w+([\\.-]?\\w+)*(\\.\\w{2,3})+$'\n",
    "\n",
    "def checkmail(email):\n",
    "    status = \"invalid\"\n",
    "    if re.search(mail_checking_regex, email):\n",
    "        status = \"valid\"\n",
    "    return status\n",
    "email=input()\n",
    "\n",
    "print(checkmail(\"\"))"
